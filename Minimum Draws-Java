import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        
        int T = in.nextInt();
        
        for(int i = 0;i<T;i++){
            int N= in.nextInt();
            System.out.println(N+1);
        }
    }
}
